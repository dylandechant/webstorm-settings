<application>
  <component name="XDebuggerSettings">
    <data-views>
      <option name="showLibraryStackFrames" value="false" />
    </data-views>
    <general />
    <debuggers>
      <debugger id="javascript">
        <configuration doNotStepIntoLibraryCode="false">
          <custom-object-presentation />
        </configuration>
      </debugger>
      <debugger id="ruby">
        <configuration>
          <option name="timeout" value="10000" />
          <option name="verboseOutput" value="true" />
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>